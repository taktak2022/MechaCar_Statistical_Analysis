# 15.7.2 LINEAR REGRESSION
?lm()
lm(qsec ~ hp,mtcars) #create linear model
# Intercept = y-INTERCEPT = 20.55635
# hp = SLOPE = -0.01846
summary(lm(qsec~hp,mtcars)) #summarize linear model
# BUILD A LINEAR MODEL - CREATE A LINE PLOT BASED ON OUTCOME
model <- lm(qsec ~ hp,mtcars) #create linear model
yvals <- model$coefficients['hp']*mtcars$hp + model$coefficients['(Intercept)']
plt <- ggplot(mtcars,aes(x=hp,y=qsec)) #import dataset into ggplot2
plt + geom_point() + geom_line(aes(y=yvals), color = "red") #plot scatter and linear model
# 15.7.3 MULTIPLE LINEAR REGRESSION MODEL
lm(qsec ~ mpg + disp + drat + wt + hp,data=mtcars) #generate multiple linear regression model
summary(lm(qsec ~ mpg + disp + drat + wt + hp,data=mtcars)) #generate summary statistics
